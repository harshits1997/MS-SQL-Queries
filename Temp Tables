--Method 2 	:100:
--Create Temp table 


CREATE TABLE #Students
(
	name VARCHAR(50),
	age int,
	gender VARCHAR (50)

)

INSERT INTO #tudents
SELECT name, age, gender
FROM student
WHERE gender = 'Male'
